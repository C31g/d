getgenv().a=true;local b=function()for c,d in ipairs(workspace.Living:GetChildren())do if string.find(d.Name,"Dio")then return true end end;return false end;local e=function()while not b()do task.wait(1)end end;task.spawn(function()e()print("Dio is fully loaded and ready for farming.")task.spawn(function()while getgenv().a do pcall(function()local f=workspace.Effects:GetChildren()for g=#f,1,-1 do local h=f[g]if h.Name=="Hold up, aint you Dio?"or h.Name=="FIRE"or h.Name=='blood'then h:Destroy()end end end)task.wait(0.2)end end)task.spawn(function()while getgenv().a do if b()then pcall(function()getgenv().i=nil;for c,d in ipairs(workspace.Living:GetChildren())do if string.find(d.Name,"Dio")then getgenv().i=d;break end end end)else task.wait(0.5)end;task.wait(0.2)end end)task.spawn(function()while getgenv().a do if getgenv().i and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Humanoid.Health>0 then pcall(function()local j=getgenv().i;local k=game.Players.LocalPlayer.Character.HumanoidRootPart;local l=j.HumanoidRootPart.Position;k.CFrame=CFrame.new(l-Vector3.new(0,0,3),l)end)end;task.wait(0.2)end end)local m=0.2;task.spawn(function()while getgenv().a do local n=game.Players.LocalPlayer.Cooldowns:FindFirstChild("Rush Attack")if n then repeat task.wait(0.1)until not game.Players.LocalPlayer.Cooldowns:FindFirstChild("Rush Attack")end;task.spawn(function()task.wait(m)local o={[1]="MouseButton1"}game:GetService("ReplicatedStorage"):WaitForChild("ReplicatedModules"):WaitForChild("KnitPackage"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("MoveInputService"):WaitForChild("RF"):WaitForChild("FireInput"):InvokeServer(unpack(o))end)local p={[1]="Spawn Boss Altar",[2]="Dio",[3]=workspace:WaitForChild("NPCSpawns"):WaitForChild("Boss Altar"):WaitForChild("Spawn"):WaitForChild("Part")}game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("NpcFunc"):InvokeServer(unpack(p))task.wait(2)end end)end)
