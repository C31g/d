local excludedOres = {"Bismuth","Goldstone","Pyrite","Black Diamond","Slate","Musgravite","Olivine","Osmium","Uranium","Emerald","Delectium","Magma", "Palladium", "Chroma Contaris", "Core Fragment", "Obsidian Glass", "Stone", "Basalt", "Amber", "Diorite", "Coal", "Crystallized Stone", "Nickel", "Bedrock", "Gold", "Granite", "Iron", "Marble", "Etherstone", "Prismatistone","Silver","Obsidian","Ice","Voidstone","Ruby","Celestone","Mantle","Barrier","Quartz","Reflectistone"}
for i,v in pairs(workspace.Mine:GetChildren()) do
if not table.find(excludedOres,v.Name) then
local args = {
    [1] = v,
    [2] = "AbleToTriggerAbilities"
}

game:GetService("ReplicatedStorage").MineOre:InvokeServer(unpack(args))
end
end
